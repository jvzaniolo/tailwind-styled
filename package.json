{
  "name": "tailwind-styled",
  "version": "0.0.1",
  "description": "Inspired by Stitches and Styled Components, Tailwind Styled is a utility function that allows you to create components with a similar syntax but using Tailwind CSS classes.",
  "main": "dist/cjs/styled.js",
  "module": "dist/esm/styled.js",
  "types": "dist/esm/styled.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-p build:*",
    "build:cjs": "tsc",
    "build:esm": "tsc --module esnext --target esnext --declaration --outDir dist/esm"
  },
  "keywords": [
    "tailwind",
    "tailwindcss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvzaniolo/tailwind-styled.git"
  },
  "bugs": {
    "url": "https://https://github.com/jvzaniolo/tailwind-styled.git/issues"
  },
  "author": "Jo√£o Vitor",
  "license": "ISC",
  "dependencies": {
    "class-variance-authority": "^0.4.0",
    "tailwind-merge": "^1.10.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "printWidth": 120,
    "endOfLine": "auto"
  }
}
